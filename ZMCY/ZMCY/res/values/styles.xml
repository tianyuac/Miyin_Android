<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="Theme_Dialog_NoTitle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="main_head_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:onClick">onClick</item>
    </style>
    
    <style name="main_head_title_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:gravity">center</item>
        <item name="android:onClick">onClick</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/selector_button_1</item>
    </style>
    
    <style name="main_head_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:drawableLeft">@drawable/ic_logo</item>
        <item name="android:drawablePadding">1.0dip</item>
    </style>
    
    <style name="sliding_left_textview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:drawablePadding">5.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">onClick</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_button_sliding_left_textview</item>
    </style>
    
    <style name="member_function_button1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#c5c5c5</item>
        <item name="android:drawablePadding">5.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">onClick</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_button_sliding_left_textview</item>
    </style>
    
    <style name="member_function_button2">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#808aa5</item>
        <item name="android:drawablePadding">5.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">onClick</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_listview_item</item>
    </style>
    
    <style name="listview1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:dividerHeight">1.0dip</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:divider">@drawable/bm_dottedline</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@android:color/transparent</item>
    </style>
    
    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.0dip</item>
        <item name="android:background">@drawable/bm_dottedline</item>
    </style>
    
    <style name="edittext1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#dbd8d8</item>
        <item name="android:maxLength">20</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:ems">10</item>
        <item name="android:drawableRight">@drawable/ic_input_clear</item>
        <item name="android:background">@drawable/bg_frame</item>
    </style>
    
    <style name="newssearch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#dbd8d8</item>
        <item name="android:maxLength">20</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:ems">10</item>
        <item name="android:drawableRight">@drawable/ic_input_clear</item>
    </style>
    
    <style name="button1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:background">@drawable/selector_button_1</item>
    </style>
    
    <style name="searchkeysbutton">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:onClick">onClick</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:layout_margin">2.0dip</item>
        <item name="android:background">@drawable/selector_button_1</item>
    </style>
    
    <style name="top_category_scroll_view_item_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:gravity">center</item>
    </style>
    
     <style name="subscribe_my_tip_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/subscribe_tip_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="subscribe_seperate_line">
        <item name="android:background">@color/subscribe_seperate_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">14.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>

    <style name="subscribe_flow_layout">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="subscribe_more_tip_text" parent="@style/subscribe_my_tip_text">
        <item name="android:textSize">13.0sp</item>
    </style>
    
</resources>