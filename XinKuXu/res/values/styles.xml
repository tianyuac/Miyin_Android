        <item name="android:button">@null</item>
        <item name="android:drawableLeft">@drawable/selector_checkbox</item>
        <item name="android:drawablePadding">4.0dip</item>
    </style>
    
    <style name="main_function_selector_bg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">onClick</item>
        <item name="android:padding">5.0dip</item>
    </style>
    <style name="test_words_page_text_selector">
        <item name="android:layout_width">400.dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">#343434</item>
