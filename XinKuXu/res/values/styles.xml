        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:layout_margin">10.0dip</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_gravity">right</item>
    </style>
    <style name="test_words_page_text_right_selector" parent="test_words_page_text_selector">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="test_words_page_text_error_selector" parent="test_words_page_text_selector">
