<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="Theme_Dialog_NoTitle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="checkbox1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#9b9ba3</item>
        <item name="android:background">@null</item>
        <item name="android:button">@null</item>
        <item name="android:checked">true</item>
        <item name="android:drawableLeft">@drawable/selector_checkbox</item>
        <item name="android:drawablePadding">4.0dip</item>
    </style>
    
    <style name="textview1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#9b9ba3</item>
    </style>
    
    <style name="textview2" parent="textview1">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#5c5cff</item>
    </style>
    
    <style name="textview3">
        <item name="android:layout_width">match_parent</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:textStyle">bold</item>
   		<item name="android:textSize">20.0dip</item>
   		<item name="android:textColor">#FFFFFF</item>
   		<item name="android:padding">5.0dip</item>
   		<item name="android:background">@drawable/bg2</item>
    </style>
    
    <style name="button1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:layout_margin">5.0dip</item>
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:onClick">onClick</item>
        <item name="android:background">@drawable/selector_button_1</item>
    </style>
    
    <style name="button2">
        <item name="android:layout_width">0.0dip</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_weight">1</item>
   		<item name="android:onClick">onClick</item>
   		<item name="android:textSize">17.0dip</item>
   		<item name="android:textColor">#FFFFFF</item>
   		<item name="android:layout_margin">5.0dip</item>
   		<item name="android:padding">8.0dip</item>
   		<item name="android:background">@drawable/selector_button_1</item>
    </style>
    
    <style name="button4">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#888888</item>
        <item name="android:onClick">onClick</item>
        <item name="android:background">@drawable/selector_button_10</item>
    </style>
    
    <style name="imageButton1">
        <item name="android:layout_width">0.0dip</item>
   		<item name="android:layout_height">wrap_content</item>
   		<item name="android:layout_weight">1</item>
   		<item name="android:layout_margin">0.5dip</item>
   		<item name="android:padding">5.0dip</item>
   		<item name="android:background">@drawable/selector_button_4</item>
    </style>
    
    <style name="edittext1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#dbd8d8</item>
        <item name="android:maxLength">20</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:ems">10</item>
        <item name="android:drawableRight">@drawable/ic_input_clear</item>
        <item name="android:background">@drawable/bg_frame</item>
    </style>
    
    <style name="listview1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:divider">@drawable/bm_dottedline_bg</item>
        <item name="android:dividerHeight">1.0dip</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:listSelector">@color/transparent</item>
    </style>
    
</resources>